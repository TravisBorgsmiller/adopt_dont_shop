<h1>Create new shelter</h1>

<main>
  <section>
    <%= form_tag "/shelters" do %>
      <%= label_tag :name %>
      <%= text_field_tag :name %><br/>
      <%= label_tag :address %>
      <%= text_field_tag :address %><br/>
      <%= label_tag :city %>
      <%= text_field_tag :city %><br/>
      <%= label_tag :state %>
      <%= text_field_tag :state %><br/>
      <%= label_tag :zip %>
      <%= text_field_tag :zip %>
      <%= label_tag :submit, "" %>
      <%= submit_tag 'Submit' %>
    <% end %>
  </section>
</main>
